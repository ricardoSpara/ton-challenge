  "paths": {
    "/users": {
      "post": {
        "tags": [
          "User"
        ],
        "summary": "Criar um usuário",
        "description": "Criar um novo usuário",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["full_name", "email", "number", "amount", "password"],
                "properties": {
                  "full_name": {
                    "type": "string"
                  },
                  "cpf": {
                    "type": "string",
                    "description": "Esse campo deve ser enviado quando o usuario for do tipo 'common'"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password":{
                    "type": "string"
                  }
                },
                "example": {
                  "full_name": "User fullname sample",
                  "cpf": "074.970.610-46",
                  "email": "weyeg60069@mxcdd.com",
                  "password": "123456"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      },
      "get": {
        "tags": [
          "User"
        ],
        "summary": "Lista todos usuários",
        "description": "Lista todos usuários",
        "responses": {
          "200": {
            "description": "Success"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/users/{userId}": {
      "get": {
        "summary": "Lista um usuário",
        "description": "Lista um usuário",
        "tags": [
          "User"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "required": true,
            "description": "ID do usuário",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/accesses": {
      "post": {
        "tags": [
          "Accesses"
        ],
        "summary": "Incrementa um acesso no site",
        "description": "Incrementa um acesso no site",
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  }
